\begin_layout Standard
\begin_inset listings
lstparams "basicstyle={\ttfamily},language=R"
inline false
status open

\begin_layout Plain Layout

remove_outliers <- function(x, na.rm = TRUE, ...) {
\end_layout

\begin_layout Plain Layout

	qnt <- quantile(x, probs=c(.01, .99), na.rm = na.rm, ...)
\end_layout

\begin_layout Plain Layout

	H <- 1.5 * IQR(x, na.rm = na.rm)
\end_layout

\begin_layout Plain Layout

	y <- x   y[x < (qnt[1] - H)] <- NA
\end_layout

\begin_layout Plain Layout

	y[x > (qnt[2] + H)] <- NA   y } 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

ant17 <- apply(ant17, 2, function(row){remove_outliers(row)})
\end_layout

\end_inset


\end_layout

